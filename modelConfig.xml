<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

  <settings>
    <setting name="jdbcTypeForNull" value="NULL"/>
    <setting name="mapUnderscoreToCamelCase" value="true"/>
  </settings>

  <typeAliases>
    <typeAlias type="kr.or.mes2.dto.DashboardDTO" alias="DashboardDTO"/>
    <typeAlias type="kr.or.mes2.dto.UserDTO" alias="UserDTO"/>
    <typeAlias type="kr.or.mes2.dto.CodeDTO" alias="CodeDTO"/>
    <typeAlias type="kr.or.mes2.dto.ItemMasterDTO" alias="ItemMasterDTO"/>
    <typeAlias type="kr.or.mes2.dto.BomDTO" alias="BomDTO"/>
    <typeAlias type="kr.or.mes2.dto.RoutingDTO" alias="RoutingDTO"/>
    <typeAlias type="kr.or.mes2.dto.EquipmentInfoDTO" alias="EquipmentInfoDTO"/>
    <typeAlias type="kr.or.mes2.dto.ProductionOrderDTO" alias="ProductionOrderDTO"/>
  </typeAliases>

  <mappers>
    <mapper resource="mybatis/mappers/DashboardMapper.xml"/>
    <mapper resource="mybatis/mappers/UserMapper.xml"/>
    <mapper resource="mybatis/mappers/CodeMapper.xml"/>
    <mapper resource="mybatis/mappers/ItemMasterMapper.xml"/>
    <mapper resource="mybatis/mappers/BomMapper.xml"/>
    <mapper resource="mybatis/mappers/RoutingMapper.xml"/>
    <mapper resource="mybatis/mappers/ProductionOrderMapper.xml"/>
  </mappers>

</configuration>
